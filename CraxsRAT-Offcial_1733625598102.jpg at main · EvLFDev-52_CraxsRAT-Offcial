input[type="checkbox"].FormControl-checkbox { appearance: none; border-color: var(--control-borderColor-emphasis); border-radius: var(--borderRadius-small); cursor: pointer; display: grid; height: var(--base-size-16); margin-right: 0px; margin-bottom: 0px; margin-left: 0px; margin-top: var(--base-size-2); place-content: center; position: relative; transition: background-color, border-color 80ms cubic-bezier(0.33, 1, 0.68, 1); width: var(--base-size-16); }

